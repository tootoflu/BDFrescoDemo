<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.bytedance.fresco.showsample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ShowSampleApplication"
        android:allowBackup="false"
        android:icon="@drawable/fresco_icon"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/FrescoAppTheme">
        <activity android:name=".SrDetailSameSizeActivity"
            android:label="@string/activity_sr_raw_same_size"
            ></activity>
        <activity
            android:name=".FigureDetailActivity"
            android:label="@string/fresco_title_activity_detail"
            android:screenOrientation="portrait"
            android:theme="@style/FrescoAppTheme"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".SrDetailActivity"
            android:label="@string/activity_sr_detail" />
        <activity
            android:name=".AuthorizationActivity"
            android:label="@string/activity_authorization" />
        <activity
            android:name=".BigFigureActivity"
            android:label="@string/activity_big_figure" />
        <activity
            android:name=".LogDetailActivity"
            android:label="@string/activity_log_detail"
            android:theme="@style/FrescoAppTheme" />
        <activity
            android:name=".CloudControlActivity"
            android:label="@string/activity_cloud_control_config" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/FrescoAppNoActionBarTheme"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/FrescoSplashTheme"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TestActivity"></activity>
        <activity android:name=".Test1Activity"></activity>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/fresco_title_activity_settings"
            android:screenOrientation="portrait"
            android:theme="@style/FrescoAppTheme"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".case.HeifEncodeActivity"
            android:label="@string/activity_heif_encode"
            android:screenOrientation="portrait"
            android:theme="@style/FrescoAppTheme"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".case.CaseActivity"
            android:label="Case"
            android:screenOrientation="portrait"
            android:theme="@style/FrescoAppTheme"
            tools:ignore="LockedOrientationActivity" />
    </application>

</manifest>